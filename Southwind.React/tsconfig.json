{
  "compilerOptions": {
    "target": "es5",
    "sourceMap": false,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "*": [
        "../Framework/Signum.React/node_modules/@types/*",
        "../Framework/Signum.React/node_modules/*",
        "*"
      ],
      "@framework/*": [
        "../Framework/Signum.React/Scripts/*"
      ],
      "@extensions/*": [
        "../Extensions/Signum.React.Extensions/*"
      ]
    },
    "lib": [
      "dom",
      "esnext"
    ]
  },
  "include": [
    "App"
  ],
  "exclude": [
    "node_modules"
  ]
}
