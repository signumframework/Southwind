{
  "name": "southwind",
  "version": "1.0.0",
  "description": "Example LOB application using Signum Framework and React",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./wwwroot/dist",
    "build:dll": "webpack --config webpack.config.dll.js",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "yarn run clean && yarn run build:dll && yarn run build:webpack",
    "watch": "yarn run clean && yarn run build:dll && webpack --config webpack.config.js --watch --progress"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signumsoftware/southwind"
  },
  "keywords": [
    "Southwind",
    "Signum Framework",
    "LINQ",
    "React"
  ],
  "author": "Signum Software",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/signumsoftware/southwind/issues"
  },
  "homepage": "https://github.com/signumsoftware/southwind",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.8",
    "@fortawesome/free-regular-svg-icons": "5.5.0",
    "@fortawesome/free-solid-svg-icons": "5.5.0",
    "@fortawesome/react-fontawesome": "0.1.3",
    "@types/react": "file:../Framework/Signum.React/node_modules/@types/react",
    "abortcontroller-polyfill": "1.1.9",
    "bootstrap": "4.1.3",
    "bpmn-js": "2.5.2",
    "diagram-js-minimap": "1.3.0",
    "codemirror": "5.42.0",
    "d3": "5.7.0",
    "d3-scale-chromatic": "1.3.3",
    "es6-promise": "4.2.5",
    "es6-object-assign": "1.1.0",
    "history": "4.7.2",
    "jquery": "3.3.1",
    "lodash-compat": "3.10.2",
    "moment": "2.22.2",
    "moment-duration-format": "2.2.2",
    "numbro": "2.1.1",
    "popper.js": "1.14.6",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react": "16.6.3",
    "react-popper": "0.10.4",
    "react-dom": "16.6.3",
    "react-overlays": "0.8.3",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-rte": "0.16.1",
    "react-widgets": "4.2.6",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "4.0.0",
