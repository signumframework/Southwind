    "raw-loader": "0.5.1",
    "rimraf": "2.6.2",
    "style-loader": "0.23.0",
    "ts-loader": "5.2.1",
    "typescript": "3.1.1",
    "url-loader": "1.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-notifier": "1.6.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "watch"
    ]
  }
}
